{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@configs": ["configs"],
      "@controllers": ["controllers"],
      "@migrations": ["migrations"],
      "@plugins": ["plugins"],
      "@service": ["service"],
      "@test": ["test"],
      "@utils": ["utils"],
      "@types": ["types"],
      "@errors": ["errors"],
      "@enums": ["enums"]
    },
    "module": "CommonJS",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "sourceMap": true
  }
}
